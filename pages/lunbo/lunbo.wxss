/* pages/home/home.wxss */
page{
    height:100%;
    background:#000;
}
.header .logo-box{
  width:130rpx;
  height:54rpx;
}
.header .logo-box .logo{
    width:100%;
    height:100%
}
.header .info{
    position: relative;
}
.header .info .info-box{
  width:50rpx;
  height:50rpx;
}
.header .info .info-text{
    position: absolute;
    left:10rpx;
    top:5rpx;
    text-align: center;
    font-size:12px;
    font-weight: 600
}
.header{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding:50rpx;
}
.title{
    width:75%;
    padding:20rpx 50rpx;
}
.title .title-text{
    width:100%;
}
.swiper-box{
    position: relative;
    
}
.swiper-box swiper{
    width:530rpx;
    height:543rpx;
    overflow: visible;
    position: absolute;
    top:120rpx;
    right: 45rpx;
}
.swiper-box swiper swiper-item{
    height:543rpx;
    overflow: visible;
    background: #fff;
}

.swiper-box .coupon{
    background:#434343;
    width:100%;
    height:111rpx;
    display:flex;
    justify-content: space-between;
    margin:0;
}
.swiper-box .coupon .one{
    display:flex;
    flex-direction: column;
    padding:15rpx;
}
.swiper-box .coupon .one text:first-child{
    color:#fff;
}
.swiper-box .coupon .one text:last-child{
    color:#aaa;
    font-size:12px;
}
.swiper-box .coupon .sjk{
    width:174rpx;
    height:100%;
    margin:0;
}
.swiper-box .coupon .two{
    width:174rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.swiper-box .coupon .two text{
    color:#fff;
    font-size:26px;
    padding:10rpx 30rpx;
    border-left:1rpx solid #555;
}
.swiper-box .big-img{
    display:block;
    width:100%;
    height:320rpx
}
.divider{
    background:#ccc;
    height:6rpx;
}
.swiper-box .member{
    padding:0 20rpx
}
.head-photo{
    position:relative;
}
.head-photo image{
    width:165rpx;
    position:absolute;
    top:40rpx;
    right:80rpx;
    z-index:1000;
}
.swiper-box .member .tit{
    padding-top:80rpx;
    font-size:26px;
    font-weight: 800
}
.swiper-box .member .pct{
    font-size:12px;
    margin-bottom:15rpx;
}
.swiper-box .member .pct text:first-child{
    font-weight:800
}
.swiper-box .member .inte{
    font-size:12px;
    padding:50rpx 0;
    border-top:1rpx solid #ccc
}
.swiper-box .member .bcode{
    font-size:12px;
}
.swiper-box .member .bcode text:first-child{
    font-weight:800
}
.swiper-box .discounts{
    padding:30rpx 50rpx;
    position:relative;
    background:rgba(0,0,0,.5);
}
.swiper-box .discounts .one{
    font-weight:800;
    font-size:26px;
}
.swiper-box .discounts .two{
    font-size:14px;
    border-top:1rpx solid #ccc;
    padding:5rpx 0
}
.swiper-box .discounts .two text{
    font-size:12px;
}
.swiper-box .discounts .barcode1{
    padding-top:200rpx;   
}
.swiper-box .discounts-a{
    padding:30rpx 50rpx;
    position: absolute;
    top:80rpx;
    left:0;
    z-index:100;
    background:#ccc
    
}
.swiper-box .discounts-a .one image{
    width:170rpx;
    height:170rpx
}
.swiper-box .discounts-a .two{
    font-size:14px;
    border-top:1rpx solid #ccc;
    padding:5rpx 0
}
.swiper-box .discounts-a .two text{
    font-size:12px;
}
.swiper-box .discounts-a .barcode2{
    padding-top:100rpx;
}
.swiper-box .discounts-b{
    padding:30rpx 50rpx;
    position: absolute;
    top:160rpx;
    left:0;
    z-index:200;
    background: #fff
}
.swiper-box .discounts-b .one{
    font-weight:800;
    font-size:26px;
}
.swiper-box .discounts-b .two{
    font-size:14px;
    border-top:1rpx solid #ccc;
    padding:5rpx 0
}
.swiper-box .discounts-b .two text{
    font-size:12px;
}
.swiper-box .discounts-b .barcode3{
    padding-top:30rpx;
}
.swiper-box .address image{
    display:block;
    width:100%;
    height:245rpx;
}
.swiper-box .address .address-card{
    display:flex;
    justify-content: space-around;
    align-items: center;
    padding-top:15rpx;
    padding-bottom:20rpx;
    margin:0 20rpx;
    border-bottom:1rpx solid #ccc
}
.swiper-box .address .address-card .add-icon{
    flex-basis: 40rpx 
}
.swiper-box .address .address-card .add-icon image{
    width:100%
}
.swiper-box .address .address-card .des{
    flex-basis:400rpx;
}
.swiper-box .address .address-card .des .store{
    font-size:14px;
    font-weight:600;
    display:flex;
    justify-content: space-between
}
.swiper-box .address .address-card .des .add{
    display: flex;
    justify-content: space-between;
    align-items: baseline;
    font-size:10px;
    color:#ccc
}
.swiper-box .address .address-card .des .add .skip{
    border-top:2rpx solid #000;
    border-right:2rpx solid #000;
    height:10rpx;
    width:10rpx;
    transform: rotate(45deg)
}
.swiper-box .act .act-img image{
    width:100%;
    height:310rpx;
}
.swiper-box .act .act-info{
    display:flex;
    justify-content: space-around;
    align-items: center
}
.swiper-box .act .act-info view:first-child{
    font-size:14px;
    font-weight:600;
    padding:20rpx;
    border-bottom: 1rpx solid #ccc
}
.swiper-box .act .act-info .square{
    width:64rpx;
    height:64rpx;
    background:#ccc;
}
.swiper-box .rec{
    padding:60rpx 30rpx 30rpx 30rpx
}
.swiper-box .rec .rec-img image{
    width:70%;
}
.swiper-box .rec .st{
    font-size:14px;
    display:flex;
    justify-content: space-between;
    padding:50rpx 0 50rpx 0;
}
.swiper-box .rec .st .sgu text,.swiper-box .rec .st .total text{
    font-size:18px;
    font-weight:600
}
.swiper-box .rec .mon{
    border-top:1rpx solid #ccc;
    border-bottom:1rpx solid #aaa;
    padding:10rpx 0;
}
.swiper-box .rec .mon .tt{
    display:flex;
    justify-content: space-between;
    font-size:12px;
    color:#555
}
.swiper-box .rec .mon .tt .mon-date,.swiper-box .rec .mon .tt .mon-price{
    display:flex;
    flex-flow: column
}
.swiper-box .rec .mon .tt .mon-date text:first-child,.swiper-box .rec .mon .tt .mon-price text:first-child{
    font-weight:600;
    font-size:16px
}
.swiper-box .rec .mon .tt .mon-price{
    align-items: flex-end
}
.swiper-box .rec .mon .divider{
    height:1rpx;
    background:#ddd;
    margin:10rpx 0
}
.swiper-box .rec .more{
    font-size:12px;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    color:#555;
}
.swiper-box .rec .more .boult{
    width:10rpx;
    height:10rpx;
    border-top:3rpx solid #555;
    border-right:3rpx solid #555;
    transform:rotate(45deg);
    margin-left:5rpx
}
.swiper-box .dots{
    position: absolute;
    top:130rpx;
    left:60rpx;
    height:543rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items:center
}
.dot{
    width:5rpx;
    height:8rpx;
    margin-bottom:10rpx;
    background:#fff;
}
.arrow{
    width:15rpx;
    height:15rpx;
    border-top:5rpx solid #fff;
    border-right:5rpx solid #fff;
    transform:rotate(135deg);
    
}
.active{
    width:5rpx;
    height:70rpx;
    margin-bottom:10rpx;
    background:#fff;
}
.arrow_top{
    width:15rpx;
    height:15rpx;
    border-top:5rpx solid #fff;
    border-left:5rpx solid #fff;
    transform:rotate(45deg);
}